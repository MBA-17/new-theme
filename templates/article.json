{
  "sections": {
    "main": {
      "type": "article",
      "blocks": {
        "featured_image": {
          "type": "featured_image",
          "settings": {
            "image_height": "large"
          }
        },
        "title": {
          "type": "title",
          "settings": {
            "title_align": "left"
          }
        },
        "share": {
          "type": "share",
          "settings": {
            "share_label": "Share",
            "share_bg": "#f7f7f8",
            "share_dc": "#eee",
            "share_alignmen": "flex-start"
          }
        },
        "content": {
          "type": "content",
          "settings": {
            "enable_dropcap": true
          }
        }
      },
      "block_order": [
        "featured_image",
        "title",
        "share",
        "content"
      ],
      "settings": {
        "enabled_image": true,
        "enabled_auth": true,
        "enabled_date": true,
        "enabled_title": true
      }
    }
  },
  "order": [
    "main"
  ]
}
